{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "NestJS Boilerplate",
  "author": "Breno Sobral",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "doc": "npx @compodoc/compodoc -p tsconfig.json",
    "doc:serve": "npm run doc && npx @compodoc/compodoc -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./__tests__/e2e/jest-e2e.json",
    "precommit": "lint-staged && npm run lint",
    "prepush": "npm run build",
    "prepare": "is-ci || husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@fastify/compress": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^6.12.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/platform-fastify": "^10.4.15",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/throttler": "^6.4.0",
    "@types/cache-manager": "^5.0.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "bullmq": "^5.40.0",
    "cache-manager": "^6.4.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-color": "^2.0.4",
    "compression": "^1.7.5",
    "fastify": "^5.2.1",
    "git-cz": "^4.9.0",
    "helmet": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "nest-commander": "^3.15.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@prisma/client": "^6.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cli-color": "^2.0.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.8.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.0.0",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write .",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
